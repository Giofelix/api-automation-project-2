				{
					"name": "Auth - CreateToken",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// 1. Convertir la respuesta a un objeto JSON\r",
									"const respuesta = pm.response.json();\r",
									"\r",
									"// 2. Extraer el valor del token (ajusta 'token' al nombre real de tu campo)\r",
									"// Si el token est√° dentro de un objeto, usa: respuesta.data.token\r",
									"const miToken = respuesta.token || respuesta.accessToken;\r",
									"\r",
									"// 3. Validar que el token no sea nulo antes de guardarlo\r",
									"pm.test(\"Token recibido correctamente\", function () {\r",
									"    pm.expect(miToken).to.not.be.undefined;\r",
									"    pm.expect(miToken).to.not.be.null;\r",
									"});\r",
									"\r",
									"// 4. Guardar el valor en una variable GLOBAL\r",
									"if (miToken) {\r",
									"    pm.globals.set(\"mi_token_global\", miToken);\r",
									"    console.log(\"Token guardado exitosamente: \" + miToken);\r",
									"}\r",
									"\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time is less than 300ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(300);\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"username\" : \"admin\",\r\n    \"password\" : \"password123\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "https://restful-booker.herokuapp.com/auth",
