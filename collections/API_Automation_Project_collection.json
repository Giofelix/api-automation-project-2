					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse the JSON response array",
									"const responseArray = pm.response.json();",
									"",
									"// Generate a random index based on the array length",
									"const randomIndex = Math.floor(Math.random() * responseArray.length);",
									"",
									"// Extract the bookingid from the randomly selected element",
									"const randomBookingId = responseArray[randomIndex].bookingid;",
									"",
									"// Save the random bookingid to the global variable \"Id_registro\"",
									"pm.globals.set(\"Id_registro\", randomBookingId);",
									"",
									"console.log(\"Selected random bookingid: \" + randomBookingId + \" from index: \" + randomIndex);",
									"",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response time is less than 500ms\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(500);",
									"});"
								],
